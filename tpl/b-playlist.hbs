{{! <div class="b-playlist"> }}
<div class="b-track-form">
    <div class="b-input b-input_search">
        <input class="b-input__text" name="search" type="text"/>
    </div>
</div>
<!--
<span class="b-playlist__title">{{title}}</span>
<span class="b-playlist__description">{{description}}</span>
-->
<ol class="b-playlist__tracks">
    {{#each tracks}}
    <li class="b-track">
        {{#if this.artwork_url}}
        <img class="b-track__artwork" src="{{this.artwork_url}}"/>
        {{/if}}
        <span class="b-track__author">{{this.user.username}}</span>
        <span class="b-track__title">{{this.title}}</span>
    </li>
    {{/each}}
</ol>
{{! </div> }}
