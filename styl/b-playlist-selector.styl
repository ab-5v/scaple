.b-playlist-selector
  padding: 0;
  margin: 0 40px;
  height: 32px;
  line-height: 32px;

  text-align: center;

  .b-playlist-selector__item
    display: inline-block;
    list-style-type: 0;
    width: 10px;
    height: 10px;
    margin: 0 5px;

    font-size: 0;
    line-height: 32px;
    background-color: #000;
    border-radius: 5px;

    cursor: pointer;
